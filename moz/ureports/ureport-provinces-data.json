{
    "label": "U-Reporters/ Utilizadores SMS BIZ - Provincia",
    "source": {
        "type": "vector",
        "layer": "name_province",
        "url": "mapbox://ona.bvmunedl",
        "data": "https://raw.githubusercontent.com/onaio/cycloneidai-2019-data/master/moz/ureports/ureport_data_province_clean.csv",
        "join": [
            "Provincia",
            "province"
        ]
    },
    "type": "fill",
    "circle-opacity": 0.4,
    "property": "people_contacted",
    "categories": {
        "breaks": "yes",
        "color": "Oranges",
        "clusters": 3
    },
    "popup": {
        "header": "{{province}}",
        "body": "<p style=text-align:left;><b>Pessoas Contatadas: </b>{{people_contacted}}<p>"
    },
    "credit": "Ureports"
}
