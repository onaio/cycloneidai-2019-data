{
    "label": "Famílias e Pessoas Afetadas",
    "source": {
      "type": "vector",
      "layer": "mozambique_districts_final",
      "url": "mapbox://ona.9zeb0vb0",
      "data": "https://raw.githubusercontent.com/onaio/cycloneidai-2019-data/master/moz/cenoa-base/cenoa_base_de_dados_2.csv",
      "minzoom": 2,
      "join": [
        "Distrito",
        "distrito"
      ]
    },
    "type": "fill",
    "property": "familias__afectadas_",
    "categories": {
        "breaks": "yes",
        "color": "Blues",
        "vector-prop": "Distrito",
        "clusters": 7
      },
    "aggregate": {
        "type": "sum",
        "group-by": "distrito",
        "date-by": "epoca",
        "timeseries": {
            "field": "period"
        },
        "extraProps": [
            "epoca",
            "familias__afectadas_",
            "pessoas_afectadas_"
        ]
    },
    "popup": {
        "join": [
            "distrito",
            "Distrito"
        ],
        "header": "district",
        "body": "<p style=text-align:left;> <b>Famílias Afetadas:</b> {{familias__afectadas_}} <br> <b>Pessoas Afetadas:</b> {{pessoas_afectadas_}} <span style=float:right;>"
    },
    "visible": false,
    "menu-credit": "INGC CENOE",
    "credit": "<a href=https://docs.google.com/spreadsheets/d/1UgkivprveuErB1WXprPL9LemI_a_gFBfYfcG2xM7x7M/edit#gid=283168942 target=_blank data-credit=credit>INGC CENOE</a>",
    "category": "Cumulative Distribution"
  }