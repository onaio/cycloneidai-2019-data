{
    "label": "População Total - Posto",
    "source": {
        "type": "vector",
        "data": "https://raw.githubusercontent.com/onaio/cycloneidai-2019-data/master/moz/population-posto/census_popultaion_posto.csv",
        "layer": "name_posto",
        "url": "mapbox://ona.6bqpcbs2",
        "join": [
            "Posto",
            "posto"
        ]
    },
    "type": "fill",
    "timestamp": "period",
    "property": "population",
    "categories": {
        "breaks": "yes",
        "color": "Reds",
        "clusters": 5
    },
    "popup": {
        "header": "posto",
        "body": " <p style=text-align:left;> <b>População:</b> {{population}}</p>"
    },
    "aggregate": {
        "timeseries": {
            "field": "period"
        }
    },
    "visible": false,
    "menu-credit": "National Bureau of Statistics Mozambique",
    "credit": "<a href=https://geonode.ingc.gov.mz/layers/geonode:pop_post_ine target=_blank data-credit=credit>INGC - GeoNode</a>",
    "category": "População"
}