{
    "label": "População Total - Posto",
    "source": {
        "type": "vector",
        "data": "https://raw.githubusercontent.com/onaio/cycloneidai-2019-data/master/moz/population-posto/census_popultaion_posto.csv",
        "layer": "posto_level_pop",
        "url": "mapbox://ona.5mqwqx53",
        "join": [
            "POSTO",
            "Posto"
        ]
    },
    "type": "fill",
    "property": "population",
    "categories": {
        "breaks": "yes",
        "color": "Reds",
        "clusters": 5
    },
    "popup": {
        "header": "posto",
        "body": " <p style=text-align:left;> <b>População:</b> {{population}}</p>"
    },
    "aggregate": {
        "timeseries": {
            "field": "period"
        }
    },
    "visible": false,
    "credit": "PNational Bureau of Statistics Mozambique",
    "category": "População"
}