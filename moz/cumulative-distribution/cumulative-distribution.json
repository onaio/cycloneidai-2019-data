{
    "label": "Distribuição Alimentar Cumulativa",
    "source": {
      "type": "vector",
      "layer": "mozambique_districts_final",
      "url": "mapbox://ona.9zeb0vb0",
      "data": "https://raw.githubusercontent.com/onaio/cycloneidai-2019-data/master/moz/cumulative-distribution/cumulative_distribution.csv",
      "minzoom": 2,
      "join": [
        "Distrito",
        "district"
      ]
    },
    "type": "fill",
    "property": "families",
    "categories": {
        "breaks": "yes",
        "color": "Blues",
        "vector-prop": "Distrito",
        "clusters": 7
      },
    "aggregate": {
        "type": "sum",
        "group-by": "district",
        "date-by": "end_date_of_distribution",
        "timeseries": {
            "field": "period"
        },
        "extraProps": [
            "families",
            "district",
            "people",
            "end_date_of_distribution",
            "children_under_5"
        ]
    },
    "popup": {
        "join": [
            "district",
            "Distrito"
        ],
        "header": "district",
        "body": "<p style=text-align:left;> <b>Families:</b> {{families}} <br> <b>People:</b> {{people}} <br> <b>Children 5:</b> {{children_under_5}} <br> <span style=float:right;>"
    },
    "visible": false,
    "credit": "Cumulative Distribution",
    "category": "Cumulative Distribution"
  }