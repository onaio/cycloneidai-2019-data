{
    "label": "População Total - Distrito",
    "source": {
        "type": "vector",
        "data": "https://raw.githubusercontent.com/onaio/cycloneidai-2019-data/master/moz/population-distrito/census_population_district.csv",
        "layer": "mozambique_districts_final",
        "url": "mapbox://ona.9zeb0vb0",
        "join": [
            "Distrito",
            "distrito"
        ]
    },
    "type": "fill",
    "property": "population",
    "categories": {
        "breaks": "yes",
        "color": "Reds",
        "clusters": 5
    },
    "popup": {
        "header": "distrito",
        "body": " <p style=text-align:left;> <b>População:</b> {{population}}</p>"
    },
    "aggregate": {
        "timeseries": {
            "field": "period"
        }
    },
    "visible": false,
    "credit": "National Bureau of Statistics Mozambique",
    "category": "População"
}